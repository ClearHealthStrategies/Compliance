{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.rulers": [80],
  "editor.codeActionsOnSave": {
    "source.fixAll.esbenp.prettier-vscode": "explicit"
  },
  "prettier.proseWrap": "always",
  "cSpell.words": [
    "ARRA",
    "Clin",
    "HITECH",
    "Majeure",
    "Optum",
    "Unpermitted",
    "Whistleblowers",
    "Whistleblowing",
    "Witnesseth"
  ]
}
